      "@id": "tri:question/facility_contact_name",
      "@type": "tri:Question",
      "key": "facility_contact_name",
      "label": "Who is the TRI reporting lead for {{facilityName}}?",
      "helper": "Name of the person coordinating data collection and submission.",
      "answerType": "text",
      "required": true
    },
    {
      "@id": "tri:question/facility_contact_email",
      "@type": "tri:Question",
      "key": "facility_contact_email",
      "label": "TRI reporting lead email",
      "answerType": "text",
      "required": true
    },
    {
      "@id": "tri:question/facility_contact_phone",
      "@type": "tri:Question",
      "key": "facility_contact_phone",
      "label": "TRI reporting lead phone number",
      "answerType": "text",
      "required": true
    },
    {
      "@id": "tri:question/verify_facility_profile",
      "@type": "tri:Question",
      "key": "verify_facility_profile",
      "label": "Confirm facility profile details for {{facilityName}} are accurate",
      "helper": "Address, NAICS code, and parent company are correct.",
      "answerType": "checkbox",
      "required": true
    },
    {
      "@id": "tri:question/verify_reporting_year",
      "@type": "tri:Question",
      "key": "verify_reporting_year",
      "label": "Confirm reporting year {{reportingYear}} ({{reportName}})",
      "helper": "If incorrect, contact corporate to align on the reporting cycle.",
      "answerType": "checkbox",
      "required": true
    },
    {
      "@id": "tri:question/threshold_applicability",
      "@type": "tri:Question",
      "key": "threshold_applicability",
      "label": "Did {{facilityName}} exceed the TRI threshold for any listed chemical (≥ {{threshold}} lbs)?",
      "helper": "Consider manufactured, processed, or otherwise used quantities for the reporting year.",
      "answerType": "yesno",
      "required": true
    },
    {
      "@id": "tri:question/chemical_list_confirmed",
      "@type": "tri:Question",
      "key": "chemical_list_confirmed",
      "label": "Confirm the list of TRI chemicals and CAS numbers is complete",
      "helper": "Include all reportable chemicals for the facility—even if releases are zero.",
      "answerType": "checkbox",
      "required": true
    },
    {
      "@id": "tri:question/calculation_method",
      "@type": "tri:Question",
      "key": "calculation_method",
      "label": "How were releases calculated/estimated?",
      "helper": "Examples: continuous emissions monitoring, mass balance, engineering calculations, supplier data, etc.",
      "answerType": "text",
      "required": true
    },
    {
      "@id": "tri:question/controls_changes",
      "@type": "tri:Question",
      "key": "controls_changes",
      "label": "Were there any process or control changes that materially affected releases?",
      "helper": "If yes, describe the change and when it occurred (e.g., new scrubber, process optimization).",
      "answerType": "text"
    },
    {
      "@id": "tri:question/waste_mgmt_summary",
      "@type": "tri:Question",
      "key": "waste_mgmt_summary",
      "label": "Summarize waste management methods used (recycling/treatment/disposal)",
      "answerType": "text"
    },
